--

	gpioTable OBJECT-TYPE 
		SYNTAX SEQUENCE OF GPIOEntry 
		MAX-ACCESS not-accessible 
		STATUS current 
		DESCRIPTION 
			"This table configures the general purpose out signals.
            
            Index 1-8 describe the general purpose output (GPO) signals GPO1-GPO8.
            Index 9 describes the standard PPS output signal referred to as OUT1 in
            the connector pinout description.
            
            Note that GPO1 and GPO2 can be output on the RS422 interfaces as well
            if those interfaces are set up for that (see OID serialPorts).
            
            Note: It will only be possible to configure I/O if this feature is available."
	::= { gpio 1 } 
			
	gpioEntry OBJECT-TYPE 
		SYNTAX GPIOEntry 
		MAX-ACCESS not-accessible 
		STATUS current 
		DESCRIPTION 
			"" 
		INDEX { gpioIndex } 
	::= { gpioTable 1 } 
	
	GPIOEntry ::= SEQUENCE { 
		gpioIndex 
		Unsigned32,
        output
        INTEGER,
		signal 
		INTEGER,
        enabled
        TruthValue
	}

	gpioIndex OBJECT-TYPE 
		SYNTAX Unsigned32 
		MAX-ACCESS not-accessible 
		STATUS current 
		DESCRIPTION 
		"Index for the table." 
		DEFVAL { 1 } 
		::= { gpioEntry 1 }
        
   	output OBJECT-TYPE
		SYNTAX  INTEGER {   
            gpo1 (0),
            gpo2 (1),
            gpo3 (2),
            gpo4 (3),
            gpo5 (4),
            gpo6 (5),
            gpo7 (6),
            gpo8 (7),
            pps (8)
        }
		MAX-ACCESS read-only 
		STATUS current 
		DESCRIPTION 
			"The output signal pin where the signal will be applied."
		::= { gpioEntry 2 }
		
	signal OBJECT-TYPE
		SYNTAX  INTEGER {   
            pps (0),
            irigb-002 (1),
            clk100Hz (2)
        }
		MAX-ACCESS read-write 
		STATUS current 
		DESCRIPTION 
			"pps: The pulse per second (PPS) signal is output on the GPO pin.
            irigb-002: IRIG-B002 (DC) is output on the GPO pin.
            clk100Hz: A 100 Hz signal is output on the GPO pin."
		::= { gpioEntry 3 }
        
    enabled OBJECT-TYPE
	SYNTAX  TruthValue
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "True if the GPO signal is enabled, False otherwise."
    ::= { gpioEntry 4 }

