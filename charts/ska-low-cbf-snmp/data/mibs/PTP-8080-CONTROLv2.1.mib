--  
-- UDP filter configuration
--

	udpFilterConfigTable OBJECT-TYPE 
		SYNTAX SEQUENCE OF UDPFilterConfigEntry 
		MAX-ACCESS not-accessible 
		STATUS current 
		DESCRIPTION 
			"This table configures the UDP filters.
            
            Note: This will only work if the UDP filtering feature is enabled." 
	::= { udpFilter 9 } 
			
	udpFilterConfigEntry OBJECT-TYPE 
		SYNTAX UDPFilterConfigEntry 
		MAX-ACCESS not-accessible 
		STATUS current 
		DESCRIPTION 
			"" 
		INDEX { udpFilterIndex } 
	::= { udpFilterConfigTable 1 } 
	
	UDPFilterConfigEntry ::= SEQUENCE { 
		udpFilterIndex 
		Unsigned32, 
		udpFilterMode 
		INTEGER,
		udpFilterField
		INTEGER,
		udpFilterDestPort
		INTEGER,
        udpFilterEnable
        INTEGER
	}
	  
	udpFilterIndex OBJECT-TYPE 
		SYNTAX Unsigned32 
		MAX-ACCESS not-accessible 
		STATUS current 
		DESCRIPTION 
		"Index for the table." 
		DEFVAL { 1 } 
		::= { udpFilterConfigEntry 1 }
		
	udpFilterMode OBJECT-TYPE
		SYNTAX  INTEGER {   
            block (0),
            allow (1)
        }
		MAX-ACCESS read-write 
		STATUS current 
		DESCRIPTION 
			"Block means all UDP packets with a source/destination port number 
            matching the UDP port filter will be blocked. 
            All other packets will be allowed.
            
            Allow means all UDP packets with a source/destination port number
            matching the UDP port filter will be allowed. 
            All other packets will be blocked.
            
            Whether to filter on the UDP source port, the UDP destination port
            or both is specified in the udpFilterField."
		::= { udpFilterConfigEntry 2 } 	

	udpFilterField OBJECT-TYPE
		SYNTAX  INTEGER {   
            udpSrcPort (0),
            udpDestPort (1),
            udpSrcAndDestPort (2)
        }
		MAX-ACCESS read-write 
		STATUS current 
		DESCRIPTION 
			"udpSrcPort: The UDP source port in the packet will be 
            matched against the UDP filters.
            
            udpDestPort: The UDP destination port in the packet will be 
            matched against the UDP filters.
            
            udpSrcAndDestPort: The UDP source port in the packet will be 
            matched against the UDP filters and the destination port in
            the packet will be matched against the destination port
            specified in the udpFilterDestPort OID."
		::= { udpFilterConfigEntry 3 } 	        
		
	udpFilterDestPort OBJECT-TYPE
		SYNTAX  INTEGER (1..65535)
		MAX-ACCESS read-write 
		STATUS current 
		DESCRIPTION
			"If udpFilterField is set to udpSrcAndDestPort, the destination
            port is specified in this entry."	
		::= { udpFilterConfigEntry 4 }
        
    udpFilterEnable OBJECT-TYPE
        SYNTAX  TruthValue
        MAX-ACCESS read-write 
        STATUS current 
        DESCRIPTION
            "True if the UDP filter is enabled, False otherwise."	
        ::= { udpFilterConfigEntry 5 }
        
        
        
        
--  
