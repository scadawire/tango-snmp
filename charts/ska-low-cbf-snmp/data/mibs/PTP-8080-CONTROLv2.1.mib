	indexFilterConfigTable OBJECT-TYPE 
		SYNTAX SEQUENCE OF IndexFilterConfigEntry 
		MAX-ACCESS not-accessible 
		STATUS current 
		DESCRIPTION 
			"Index filter configuration.
            
            Note: This will only work if the index filtering feature is enabled."
	::= { indexFilter 9 }
			
	indexFilterConfigEntry OBJECT-TYPE 
		SYNTAX IndexFilterConfigEntry 
		MAX-ACCESS not-accessible 
		STATUS current 
		DESCRIPTION 
			"Entry in the index filtering config table." 
		INDEX { indexFilterConfigIndex } 
	::= { indexFilterConfigTable 1 } 
	
	IndexFilterConfigEntry ::= SEQUENCE { 
		indexFilterConfigIndex 
		Unsigned32, 
		enableEgressFilter
		TruthValue,
        egressMode
        INTEGER,
        filterType
        INTEGER,
        filterIndex
        Unsigned32,
        filterMask
        Unsigned32
	}
	  
	indexFilterConfigIndex OBJECT-TYPE 
		SYNTAX Unsigned32 
		MAX-ACCESS not-accessible 
		STATUS current 
		DESCRIPTION 
		"Index for the table." 
		DEFVAL { 1 } 
		::= { indexFilterConfigEntry 1 }
        
    enableEgressFilter OBJECT-TYPE 
		SYNTAX  TruthValue
		MAX-ACCESS read-write
		STATUS current 
		DESCRIPTION 
		"True if filters shall be matched on egress." 
		DEFVAL { 2 } 
		::= { indexFilterConfigEntry 2 }
        
    egressMode OBJECT-TYPE 
	SYNTAX  INTEGER {
            pass (1),   
            block (2)
        }
		MAX-ACCESS read-write
		STATUS current 
		DESCRIPTION 
		"Block: Block packet when match.
         Pass: Pass packet when match." 
		DEFVAL { 2 } 
		::= { indexFilterConfigEntry 3 }
               
    filterType OBJECT-TYPE 
	SYNTAX  INTEGER {   
            specifyFilterIndex (1),
            udpDestPort (2),
            udpSourcePort (3),
            udpSourceAndDestPort (4),
            ipSourcePort (5),
            ipDestPort (6)
        }
		MAX-ACCESS read-write
		STATUS current 
		DESCRIPTION 
		"1: Specify the index in the Ethernet packet (starting at 0) to 
            filter on. The filter index and mask must be specified.
            
         2: Filter on UDP destination port.
            The filter index and mask is automatically set and cannot be altered.
            
         3: Filter on UDP source port.
            The filter index and mask is automatically set and cannot be altered.
            
         4: Filter on UDP source and destination port.
            The filter index and mask is automatically set and cannot be altered.
            
         5: Filter on IP source port.
            The filter index and mask is automatically set and cannot be altered.
            
         6: Filter on IP destination port.
            The filter index and mask is automatically set and cannot be altered." 
		DEFVAL { 1 } 
		::= { indexFilterConfigEntry 4 }               
        
    filterIndex OBJECT-TYPE 
	SYNTAX  Unsigned32 (0..4294967295)
		MAX-ACCESS read-write
		STATUS current 
		DESCRIPTION 
		"Starting byte number in the packet that should be matched
         with the filter.
         Byte 0 is the most significant byte of the source MAC address." 
		DEFVAL { 0 } 
		::= { indexFilterConfigEntry 5 } 
               
    filterMask OBJECT-TYPE 
	SYNTAX  Unsigned32 (0..4294967295)
		MAX-ACCESS read-write
		STATUS current 
		DESCRIPTION 
		"Mask of the filter that must match" 
		DEFVAL { 0 } 
		::= { indexFilterConfigEntry 6 }
		
--  
-- NTP functionality
--

    ntpClientEnable OBJECT-TYPE
		SYNTAX  TruthValue
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
        "Enable or disable the NTP client"
		::= { ntp 1 }
		
	ntpClientQueryInterval OBJECT-TYPE 
	SYNTAX  Integer32
		MAX-ACCESS read-write
		STATUS current 
		DESCRIPTION 
		"This is the minimum interval between transmitted messages, in seconds
		 as a power of two. For instance, a value of six indicates a minimum
		 interval of 64 seconds. " 
		DEFVAL { 0 } 
		::= { ntp 2 }
		
	ntpServerIpAddress OBJECT-TYPE
	    SYNTAX     IpAddress
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
        "IPv4 address of the NTP server."
		::= { ntp 3 }
		
	ntpClientOffset OBJECT-TYPE 
	SYNTAX  Integer32
		MAX-ACCESS read-only
		STATUS current 
		DESCRIPTION 
		"Current NTP client offset from server in us." 		
		::= { ntp 4 }
		
END